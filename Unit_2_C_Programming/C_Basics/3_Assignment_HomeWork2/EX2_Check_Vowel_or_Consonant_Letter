#include <stdio.h>
#include <stdlib.h>

int main(void) {

	char x;
	printf("Enter an alphabet: ");
	fflush(stdin); fflush(stdout);
	scanf("%c",&x);

	switch(x)
	{
	case'a':
	case'e':
	case'o':
	case'i':
	case'u':

		printf("%c is vowel",x);

	break;
	default:
		printf("%c is constant",x);
	}

}
