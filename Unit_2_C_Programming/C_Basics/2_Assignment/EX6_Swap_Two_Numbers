#include <stdio.h>
#include <stdlib.h>

int main(void) {

	float a,b,c;
	printf("Enter value of a: ");
	fflush(stdin); fflush(stdout);
	scanf("%f",&a);
	printf("Enter value of b: ");
	fflush(stdin); fflush(stdout);
	scanf("%f",&b);
	c=b;
	b=a;
	a=c;
	printf("After swapping, value of a = %f \n",a);
	printf("After swapping, value of b = %f",b);
}
