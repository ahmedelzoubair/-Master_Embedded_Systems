#include <stdio.h>
#include <stdlib.h>

int main(void) {

	float deg [10];
	int x=0;
	int i=1;

	for(x=0;x<10;x++,i++)
	{
		printf("Enter student %d degree : ",i);
		fflush(stdin); fflush(stdout);
		scanf("%f",&deg[x]);
	}

	i=1;
	x=0;
	for(x=0;x<10;x++,i++)
	{
		printf("Student %d degree is %f \n",i,deg[x]);
	}
}
