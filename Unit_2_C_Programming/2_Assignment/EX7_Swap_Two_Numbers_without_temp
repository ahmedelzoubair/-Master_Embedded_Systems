#include <stdio.h>
#include <stdlib.h>

int main(void) {

  float a,b; // ex: a=1 and b=2
	printf("Enter value of a: ");
	fflush(stdin); fflush(stdout);
	scanf("%f",&a);
	printf("Enter value of b: ");
	fflush(stdin); fflush(stdout);
	scanf("%f",&b);
	a=a+b;	//a=1+2=3
	b=a-b;	//b=3-2=1
	a=a-b;	//a=3-1=2
	printf("After swapping, value of a = %f \n",a);
	printf("After swapping, value of b = %f",b);
  
  }
